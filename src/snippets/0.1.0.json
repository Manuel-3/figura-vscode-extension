{
  "Hide a specific model": {
    "prefix": "Hide Model",
    "body": [
      "vanilla_model.${1|ALL,PLAYER,ARMOR,ELYTRA,OUTER_LAYER,INNER_LAYER|}:setVisible(false)",
      ""
    ],
    "description": "For hiding a specific model."
  },
